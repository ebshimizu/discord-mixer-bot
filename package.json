{
  "name": "discord-mixer-bot",
  "version": "1.0.3",
  "description": "A basic sound mixer/playback system that outputs through a Discord bot.",
  "main": "src/main/main.js",
  "scripts": {
    "start": "electron .",
    "rebuild-deps": "electron-builder install-app-deps",
    "build": "electron-builder build",
    "dist": "electron-builder build --publish always"
  },
  "repository": "https://github.com/ebshimizu/discord-mixer-bot",
  "author": "Falindrith",
  "license": "MIT",
  "devDependencies": {
    "electron": "^18.3.7",
    "electron-builder": "^22.6.0",
    "vue-devtools": "^5.1.3"
  },
  "dependencies": {
    "@descript/web-audio-js": "^0.15.2",
    "@discordjs/opus": "^0.3.2",
    "discord.js": "^12.2.0",
    "easy-ffmpeg": "^0.0.15",
    "electron-store": "^5.1.1",
    "electron-updater": "^4.3.1",
    "element-ui": "^2.13.1",
    "ffmpeg-static": "^4.2.1",
    "fs-extra": "^9.0.0",
    "libsodium-wrappers": "^0.7.6",
    "speaker": "^0.5.1",
    "stream-storage": "^0.0.5",
    "uuid": "^8.0.0",
    "vue": "^2.6.11",
    "vuex": "^3.4.0",
    "vuex-electron": "ebshimizu/vuex-electron",
    "ytdl-core": "^2.1.3"
  },
  "build": {
    "appId": "com.discord-mixer-bot.app",
    "asar": false,
    "publish": [
      "github"
    ],
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    }
  }
}
